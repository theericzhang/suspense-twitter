{
    "version": 2,
    "rewrites": [
        { 
            "source": "/api/(.*)", 
            "destination": "/api" 
        }
    ],
    "builds": [
        {
            "src": "/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/api/index.js"
        }
    ]
}